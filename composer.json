{
	"name": "nette/security",
	"description": "ðŸ”‘ Nette Security: provides authentication, authorization and a role-based access control management via ACL (Access Control List)",
	"keywords": ["nette", "authentication", "authorization", "ACL"],
	"homepage": "https://nette.org",
	"license": ["BSD-3-Clause", "GPL-2.0-only", "GPL-3.0-only"],
	"authors": [
		{
			"name": "David Grudl",
			"homepage": "https://davidgrudl.com"
		},
		{
			"name": "Nette Community",
			"homepage": "https://nette.org/contributors"
		}
	],
	"require": {
		"php": "8.1 - 8.5",
		"nette/utils": "^4.0"
	},
	"require-dev": {
		"nette/di": "^3.1",
		"nette/http": "^3.2",
		"nette/tester": "^2.6",
		"tracy/tracy": "^2.9",
		"phpstan/phpstan": "^2.1@stable",
		"phpstan/extension-installer": "^1.4@stable",
		"nette/phpstan-rules": "^1.0",
		"mockery/mockery": "^1.6@stable"
	},
	"conflict": {
		"nette/di": "<3.0-stable",
		"nette/http": "<3.1.3"
	},
	"autoload": {
		"classmap": ["src/"],
		"psr-4": {
			"Nette\\": "src"
		}
	},
	"minimum-stability": "dev",
	"scripts": {
		"phpstan": "phpstan analyse",
		"tester": "tester tests -s"
	},
	"extra": {
		"branch-alias": {
			"dev-master": "3.2-dev"
		}
	},
	"config": {
		"allow-plugins": {
			"phpstan/extension-installer": true
		}
	}
}
